	<!-- Google Maps script -->
    <script>
		var mapSection; 	// This is now available to both event listeners and the initMap() function
		var mapCenter; 		// Declare a variable to store the center of the map
		var centerMarker; 	// declare your marker
		
		// API callback function
		function initMap() {
			"use strict";
			// Create options object and set properties
		    var mapOptions = {
			
		        center: new google.maps.LatLng(7.111261974981827, 125.49048114730724),
		        zoom: 12,
		        mapTypeId: google.maps.MapTypeId.ROADMAP,
		        mapTypeControl: true,
				mapTypeControlOptions: {
					style: google.maps.MapTypeControlStyle.DEFAULT,
					position: google.maps.ControlPosition.LEFT_BOTTOM
				},
				draggableCursor: 'default',
				fullscreenControl: false,
				streetViewControl: false,
		        scrollwheel: false,
		        keyboardShortcuts: false,
		    };
		
			// Initialise map 
		    mapSection = new google.maps.Map(document.getElementById("location"), mapOptions);
		
		    // Create a new marker and add it to the map
		    centerMarker = new google.maps.Marker({
		        position: new google.maps.LatLng(7.111261974981827, 125.49048114730724),
		        map: mapSection,
		        animation: google.maps.Animation.DROP
		    });
		
		    mapCenter = mapSection.getCenter(); // Assign the center of the loaded map to the variable
			
			google.maps.event.addDomListener(window, "resize", function () {
				google.maps.event.trigger(mapSection, "resize");
			    // Here set the center of the map based on your "mapCenter" variable
			    mapSection.setCenter(mapCenter); 
			});
			
			google.maps.event.addListener(mapSection,'idle',function (event) {
		    	mapCenter = mapSection.getCenter();
			});

			google.maps.event.addDomListener(window, "load", initMap);

		}
    </script>

